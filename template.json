{
  "package": {
    "dependencies": {
      "@carto/airship-api": "^3.0.0-alpha2",
      "@material-ui/core": "^4.11.0",
      "@material-ui/icons": "^4.9.1",
      "@reduxjs/toolkit": "^1.1.0",
      "@testing-library/jest-dom": "^4.2.4",
      "@testing-library/react": "^9.3.2",
      "@testing-library/user-event": "^7.1.2",
      "deck.gl": "8.3.0",
      "echarts": "^4.9.0",
      "echarts-for-react": "^2.0.16",
      "history": "^5.0.0",
      "react": "^16.13.1",
      "react-dom": "^16.13.1",
      "react-map-gl": "^5.2.8",
      "react-redux": "^7.1.3",
      "react-router-dom": "^6.0.0-beta.0",
      "react-scripts": "3.4.3",
      "eslint-config-prettier": "^6.12.0",
      "eslint-loader": "^4.0.2",
      "eslint-plugin-prettier": "^3.1.4",
      "eslint-plugin-react": "^7.21.2",
      "husky": "^4.3.0",
      "lint-staged": "^10.4.0",
      "prettier": "^2.1.2",
      "react-app-rewired": "^2.1.6"
    },
    "eslintConfig": {
      "extends": "react-app"
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    },
    "lint-staged": {
      "*.+(js|jsx)": [
        "yarn lint:fix",
        "git add"
      ],
      "*.+(js|jsx|json|css|md)": [
        "prettier --write",
        "git add"
      ]
    }
  }
