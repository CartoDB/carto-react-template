{
  "name": "carto-for-react",
  "version": "1.2.0-beta.1",
  "private": true,
  "dependencies": {
    "@carto/react-api": "^1.2.1-beta.6",
    "@carto/react-auth": "^1.2.1-beta.6",
    "@carto/react-basemaps": "^1.2.1-beta.6",
    "@carto/react-core": "^1.2.1-beta.6",
    "@carto/react-redux": "^1.2.1-beta.6",
    "@carto/react-ui": "^1.2.1-beta.6",
    "@carto/react-widgets": "^1.2.1-beta.6",
    "@carto/react-workers": "^1.2.1-beta.6",
    "@deck.gl/carto": "^8.7.0-beta.5",
    "@formatjs/intl-getcanonicallocales": "^1.7.3",
    "@formatjs/intl-locale": "^2.4.37",
    "@formatjs/intl-numberformat": "^7.2.3",
    "@formatjs/intl-pluralrules": "^4.1.3",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@nebula.gl/edit-modes": "^1.0.2-alpha.0",
    "@nebula.gl/layers": "^1.0.2-alpha.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/boolean-intersects": "^6.5.0",
    "@turf/boolean-within": "^6.5.0",
    "cypress": "^6.3.0",
    "deck.gl": "^8.7.0-beta.5",
    "deepmerge": "^4.2.2",
    "echarts": "^5.2.0",
    "echarts-for-react": "^3.0.1",
    "history": "^5.0.1",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-map-gl": "^5.3.17",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.0.0-beta.4",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.2",
    "hygen": "^6.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "react-app-rewired": "^2.1.8",
    "react-is": "^17.0.2",
    "start-server-and-test": "^1.12.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "cypress": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:ci": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start https-get://localhost:3000 cy:run",
    "link-carto-react": "yarn link @carto/react-api @carto/react-auth @carto/react-basemaps @carto/react-core @carto/react-redux @carto/react-ui @carto/react-widgets @carto/react-workers",
    "unlink-carto-react": "yarn unlink @carto/react-api @carto/react-auth @carto/react-basemaps @carto/react-core @carto/react-redux @carto/react-ui @carto/react-widgets @carto/react-workers",
    "link-deck": "yarn link @deck.gl/core @deck.gl/layers @deck.gl/geo-layers @deck.gl/google-maps @deck.gl/carto @deck.gl/extensions @deck.gl/react",
    "unlink-deck": "yarn unlink @deck.gl/core @deck.gl/layers @deck.gl/geo-layers @deck.gl/google-maps @deck.gl/carto @deck.gl/extensions @deck.gl/react",
    "lint": "eslint './**/*.{js,jsx}' --ignore-path gitignore",
    "format": "prettier --write . --ignore-path gitignore",
    "hygen": "hygen",
    "updateSupportedBrowsers": "echo \"var browserRegex = $(npx browserslist-useragent-regexp --allowHigherVersions);\" > public/supportedBrowsers.js"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true
    },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "warn",
      "no-console": "warn",
      "no-debugger": "warn"
    },
    "extends": ["react-app", "prettier"]
  },
  "prettier": {
    "printWidth": 90,
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all", "not explorer 11"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": ["yarn lint --max-warnings 0", "git add"],
    "*.+(js|jsx|json|css|md)": ["yarn format", "git add"]
  }
}
