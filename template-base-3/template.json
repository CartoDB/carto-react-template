{
  "package": {
    "dependencies": {
      "@auth0/auth0-react": "^1.7.0",
      "@carto/react-api": "^1.2.1-alpha.1",
      "@carto/react-basemaps": "^1.2.1-alpha.1",
      "@carto/react-core": "^1.2.1-alpha.1",
      "@carto/react-redux": "^1.2.1-alpha.1",
      "@carto/react-ui": "^1.2.1-alpha.1",
      "@carto/react-widgets": "^1.2.1-alpha.1",
      "@carto/react-workers": "^1.2.1-alpha.1",
      "@deck.gl/carto": "^8.7.0-alpha.11",
      "@formatjs/intl-getcanonicallocales": "^1.7.3",
      "@formatjs/intl-locale": "^2.4.37",
      "@formatjs/intl-numberformat": "^7.2.3",
      "@formatjs/intl-pluralrules": "^4.1.3",
      "@material-ui/core": "^4.12.3",
      "@material-ui/icons": "^4.11.2",
      "@material-ui/lab": "4.0.0-alpha.60",
      "@nebula.gl/edit-modes": "^1.0.2-alpha.0",
      "@nebula.gl/layers": "^1.0.2-alpha.0",
      "@reduxjs/toolkit": "^1.6.1",
      "@testing-library/jest-dom": "^5.14.1",
      "@testing-library/react": "^12.1.0",
      "@testing-library/user-event": "^13.2.1",
      "@turf/bbox-polygon": "^6.5.0",
      "@turf/boolean-intersects": "^6.5.0",
      "@turf/boolean-within": "^6.5.0",
      "deck.gl": "^8.7.0-alpha.11",
      "deepmerge": "^4.2.2",
      "echarts": "^5.2.0",
      "echarts-for-react": "^3.0.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^3.4.1",
      "history": "^5.0.1",
      "husky": "^7.0.2",
      "hygen": "^6.1.0",
      "lint-staged": "^11.1.2",
      "prettier": "^2.4.1",
      "react": "^17.0.2",
      "react-app-polyfill": "^2.0.0",
      "react-app-rewired": "^2.1.8",
      "react-dom": "^17.0.2",
      "react-is": "^17.0.2",
      "react-map-gl": "^5.3.17",
      "react-redux": "^7.2.5",
      "react-router-dom": "^6.0.0-beta.4",
      "react-scripts": "4.0.3"
    },
    "scripts": {
      "lint": "eslint './**/*.{js,jsx}' --ignore-path .gitignore",
      "format": "prettier --write . --ignore-path .gitignore",
      "hygen": "hygen",
      "updateSupportedBrowsers": "echo \"var browserRegex = $(npx browserslist-useragent-regexp --allowHigherVersions);\" > public/supportedBrowsers.js"
    },
    "eslintConfig": {
      "root": true,
      "env": {
        "browser": true
      },
      "plugins": [
        "prettier"
      ],
      "rules": {
        "prettier/prettier": "warn",
        "no-console": "warn",
        "no-debugger": "warn"
      },
      "extends": [
        "react-app",
        "prettier"
      ]
    },
    "prettier": {
      "printWidth": 90,
      "arrowParens": "always",
      "semi": true,
      "tabWidth": 2,
      "singleQuote": true,
      "jsxSingleQuote": true
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    },
    "lint-staged": {
      "*.+(js|jsx)": [
        "yarn lint --max-warnings 0",
        "git add"
      ],
      "*.+(js|jsx|json|css|md)": [
        "yarn format",
        "git add"
      ]
    }
  }
}
