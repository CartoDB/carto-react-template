      "lint-staged": "^10.5.3",
      "prettier": "^2.2.1",
      "react": "^17.0.1",
      "react-app-polyfill": "^2.0.0",
      "react-app-rewired": "^2.1.8",
      "react-dom": "^17.0.1",
      "react-is": "^17.0.1",
