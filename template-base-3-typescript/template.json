      "react-scripts": "4.0.3",
      "start-server-and-test": "^1.13.1",
      "typescript": "^4.3.5"
    },
    "scripts": {
      "lint": "eslint './**/*.{ts,tsx}' --ignore-path .gitignore",
      "format": "prettier --write . --ignore-path .gitignore",
      "hygen": "hygen",
      "updateSupportedBrowsers": "echo \"var browserRegex = $(npx browserslist-useragent-regexp --allowHigherVersions);\" > public/supportedBrowsers.js"
    },
    "eslintConfig": {
      "root": true,
      "env": {
        "browser": true
      },
      "plugins": [
        "prettier"
      ],
      "rules": {
        "prettier/prettier": "warn",
        "no-console": "warn",
        "no-debugger": "warn"
      },
      "extends": [
        "react-app",
        "prettier",
        "prettier/react"
      ]
    },
    "prettier": {
      "printWidth": 90,
      "arrowParens": "always",
      "semi": true,
      "tabWidth": 2,
      "singleQuote": true,
      "jsxSingleQuote": true
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    },
    "lint-staged": {
      "*.+(ts|tsx)": [
        "yarn lint --max-warnings 0",
        "git add"
      ],
      "*.+(ts|tsx|json|css|md)": [
        "yarn format",
        "git add"
      ]
    }
  }
}
