      "eslint-plugin-prettier": "^3.4.0",
      "history": "^5.0.0",
      "husky": "^7.0.1",
      "hygen": "^6.1.0",
      "lint-staged": "^11.1.2",
      "prettier": "^2.3.2",
      "react": "^17.0.2",
      "react-app-polyfill": "^2.0.0",
      "react-app-rewired": "^2.1.8",
      "react-dom": "^17.0.2",
      "react-is": "^17.0.2",
      "react-map-gl": "^5.3.16",
      "react-redux": "^7.2.4",
