    },
    "scripts": {
      "lint": "eslint './**/*.{ts,tsx}' --ignore-path .gitignore",
      "format": "prettier --write . --ignore-path .gitignore",
      "hygen": "hygen",
      "updateSupportedBrowsers": "echo \"var browserRegex = $(npx browserslist-useragent-regexp --allowHigherVersions);\" > public/supportedBrowsers.js"
    },
    "eslintConfig": {
      "root": true,
      "env": {
        "es6": true,
        "browser": true
      },
      "plugins": ["prettier"],
      "rules": {
        "no-console": 1,
        "no-debugger": 1,
        "prettier/prettier": 1
      },
      "extends": ["react-app", "prettier"]
    },
    "prettier": {
      "arrowParens": "always",
      "bracketSpacing": true,
      "jsxSingleQuote": true,
      "printWidth": 80,
      "semi": true,
      "singleQuote": true,
      "tabWidth": 2,
      "trailingComma": "all",
      "useTabs": false
    },
    "browserslist": {
      "production": ["defaults", "not ie 11"],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    },
    "lint-staged": {
      "*.+(ts|tsx)": ["yarn lint --max-warnings 0", "git add"],
      "*.+(ts|tsx|json|css|md)": ["yarn format", "git add"]
    }
  }
