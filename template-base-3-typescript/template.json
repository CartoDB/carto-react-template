      "arrowParens": "always",
      "bracketSpacing": true,
      "jsxSingleQuote": true,
      "printWidth": 80,
      "semi": true,
      "singleQuote": true,
      "tabWidth": 2,
      "trailingComma": "all",
      "useTabs": false
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    },
    "lint-staged": {
      "*.+(ts|tsx)": ["yarn lint --max-warnings 0", "git add"],
      "*.+(ts|tsx|json|css|md)": ["yarn format", "git add"]
    }
  }
