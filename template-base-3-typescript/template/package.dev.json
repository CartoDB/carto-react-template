{
  "name": "carto-for-react",
  "version": "1.1.0-alpha.3",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^1.5.0",
    "@carto/react-api": "1.1.0-alpha.6",
    "@carto/react-auth": "^1.0.1",
    "@carto/react-basemaps": "1.1.0-alpha.6",
    "@carto/react-core": "1.1.0-alpha.6",
    "@carto/react-redux": "1.1.0-alpha.6",
    "@carto/react-ui": "1.1.0-alpha.6",
    "@carto/react-widgets": "1.1.0-alpha.6",
    "@carto/react-workers": "1.1.0-alpha.6",
    "@deck.gl/carto": "^8.5.2",
    "@formatjs/intl-getcanonicallocales": "^1.5.3",
    "@formatjs/intl-locale": "^2.4.35",
    "@formatjs/intl-numberformat": "^7.2.1",
    "@formatjs/intl-pluralrules": "^4.1.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/boolean-intersects": "^6.5.0",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "cypress": "^8.1.0",
    "deck.gl": "^8.5.2",
    "deepmerge": "^4.2.2",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "firebase-tools": "^9.16.0",
    "history": "^5.0.0",
    "husky": "^7.0.1",
    "hygen": "^6.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-map-gl": "^5.3.16",
    "react-redux": "^7.2.4",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "4.0.3",
    "start-server-and-test": "^1.13.1",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "hygen": "hygen",
    "updateSupportedBrowsers": "echo \"var browserRegex = $(npx browserslist-useragent-regexp --allowHigherVersions);\" > public/supportedBrowsers.js",
    "cypress": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:ci": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start https-get://localhost:3000 cy:run",
    "lint": "eslint './**/*.{ts,tsx}'",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "no-console": "warn",
      "no-debugger": "warn"
    },
    "extends": [
      "react-app",
      "prettier",
      "prettier/react"
    ]
  },
  "prettier": {
    "printWidth": 90,
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not explorer 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "npm run lint -- --max-warnings 0",
      "git add"
    ],
    "*.+(ts|tsx|json|css|md)": [
      "npm run format",
      "git add"
    ]
  }
}
