{
  "name": "carto-for-react",
  "version": "1.2.0-beta.2",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^1.7.0",
    "@carto/react-api": "^1.2.1",
    "@carto/react-basemaps": "^1.2.1",
    "@carto/react-core": "^1.2.1",
    "@carto/react-redux": "^1.2.1",
    "@carto/react-ui": "^1.2.1",
    "@carto/react-widgets": "^1.2.1",
    "@carto/react-workers": "^1.2.1",
    "@deck.gl/carto": "^8.7.0",
    "@formatjs/intl-getcanonicallocales": "^1.7.3",
    "@formatjs/intl-locale": "^2.4.37",
    "@formatjs/intl-numberformat": "^7.2.3",
    "@formatjs/intl-pluralrules": "^4.1.3",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@nebula.gl/edit-modes": "^1.0.2-alpha.0",
    "@nebula.gl/layers": "^1.0.2-alpha.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/boolean-intersects": "^6.5.0",
    "@turf/boolean-within": "^6.5.0",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "deck.gl": "^8.7.0",
    "deepmerge": "^4.2.2",
    "echarts": "^4.9.0",
    "echarts-for-react": "^3.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "firebase-tools": "^9.18.0",
    "history": "^5.0.1",
    "husky": "^7.0.2",
    "hygen": "^6.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-map-gl": "^5.3.17",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.0.0-beta.4",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "eslint './**/*.{ts,tsx}' --ignore-path .gitignore",
    "format": "prettier --write . --ignore-path .gitignore",
    "hygen": "hygen",
    "updateSupportedBrowsers": "echo \"var browserRegex = $(npx browserslist-useragent-regexp --allowHigherVersions);\" > public/supportedBrowsers.js",
    "link-carto-react": "yarn link @carto/react-api @carto/react-basemaps @carto/react-core @carto/react-redux @carto/react-ui @carto/react-widgets @carto/react-workers",
    "unlink-carto-react": "yarn unlink @carto/react-api @carto/react-basemaps @carto/react-core @carto/react-redux @carto/react-ui @carto/react-widgets @carto/react-workers",
    "link-deck": "yarn link @deck.gl/core @deck.gl/layers @deck.gl/geo-layers @deck.gl/google-maps @deck.gl/carto @deck.gl/extensions @deck.gl/react",
    "unlink-deck": "yarn unlink @deck.gl/core @deck.gl/layers @deck.gl/geo-layers @deck.gl/google-maps @deck.gl/carto @deck.gl/extensions @deck.gl/react"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all", "not explorer 11"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "browser": true
    },
    "plugins": ["prettier"],
    "rules": {
      "no-console": 1,
      "no-debugger": 1,
      "prettier/prettier": 1
    },
    "extends": ["react-app", "prettier"]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": ["yarn lint --max-warnings 0", "git add"],
    "*.+(ts|tsx|json|css|md)": ["yarn format", "git add"]
  }
}
