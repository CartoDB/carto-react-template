{
  "name": "carto-for-react",
  "version": "1.0.0-beta12",
  "private": true,
  "dependencies": {
    "@carto/react": "1.0.0-beta12",
    "@formatjs/intl-getcanonicallocales": "^1.5.3",
    "@formatjs/intl-locale": "^2.4.14",
    "@formatjs/intl-numberformat": "^6.1.4",
    "@formatjs/intl-pluralrules": "^4.0.6",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "cypress": "^6.3.0",
    "deck.gl": "^8.4.0",
    "echarts": "^5.0.1",
    "echarts-for-react": "^2.0.16",
    "history": "^5.0.0",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.1",
    "react-map-gl": "^5.3.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "4.0.1",
    "web-vitals": "^1.1.0",
    "workbox-background-sync": "^6.0.2",
    "workbox-broadcast-update": "^6.0.2",
    "workbox-cacheable-response": "^6.0.2",
    "workbox-core": "^6.0.2",
    "workbox-expiration": "^6.0.2",
    "workbox-google-analytics": "^6.0.2",
    "workbox-navigation-preload": "^6.0.2",
    "workbox-precaching": "^6.0.2",
    "workbox-range-requests": "^6.0.2",
    "workbox-routing": "^6.0.2",
    "workbox-strategies": "^6.0.2",
    "workbox-streams": "^6.0.2"
  },
  "devDependencies": {
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "firebase-tools": "^9.2.2",
    "husky": "^4.3.8",
    "hygen": "^6.0.4",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8",
    "react-is": "^17.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "cypress": "npx cypress open",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{js,jsx}'",
    "lint:fix": "eslint './src/**/*.{js,jsx}' --fix",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\" --ignore-path .gitignore",
    "hygen": "hygen",
    "updateSupportedBrowsers": "echo \"var browserRegex = $(npx browserslist-useragent-regexp --allowHigherVersions);\" > public/supportedBrowsers.js"
  },
  "eslintConfig": {
    "extends": ["react-app", "prettier", "prettier/react"]
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all", "not explorer 11"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": ["yarn lint:fix", "git add"],
    "*.+(js|jsx|json|css|md)": ["prettier --write", "git add"]
  }
}
